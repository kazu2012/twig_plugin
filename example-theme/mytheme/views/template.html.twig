{#
	template.html.twig - Here put your 'schema', how to show the informations
#}

{% extends 'getsimple.html.twig' %}

{% block body %}
	<header class="jumbo">
		<h1>{{ page.title }}</h1>
	</header>

	<nav>
		{% for item in site.menu %}
			<li><a href="{{ item.url }}" >{{ item.menu_text }}</a></li>
		{% endfor %}
	</nav>

	<section class="content" >
		{{ page.content|raw }}
	</section>
{% endblock %}